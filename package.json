{
  "name": "frontuna-app",
  "version": "1.0.0",
  "description": "AI-powered component generation platform",
  "scripts": {
    "start": "cd frontend && ng serve",
    "build": "cd frontend && ng build",
    "backend": "cd backend && node server-production.js",
    "bug-scan": "node backend/ai-bug-guardian.js",
    "auto-scan": "node backend/auto-bug-scanner.js",
    "health-check": "npm run bug-scan && echo 'System health check complete'",
    "fix-guide": "npm run bug-scan && echo 'Check DATABASE-FIX-GUIDE.md for step-by-step fixes'"
  },
  "keywords": ["ai", "components", "angular", "frontend", "development"],
  "author": "Frontuna Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}