{
  "name": "frontuna-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "node scripts/test-runner.js",
    "test:basic": "ng test --watch=false --browsers=ChromeHeadless",
    "test:watch": "ng test",
    "test:ci": "ng test --configuration=ci",
    "test:coverage": "ng test --configuration=coverage",
    "test:debug": "ng test --configuration=debug",
    "test:coverage-open": "npm run test:coverage && npm run coverage:open",
    "coverage:open": "node -e \"require('child_process').exec(process.platform === 'win32' ? 'start' : process.platform === 'darwin' ? 'open' : 'xdg-open', ['coverage/frontuna-frontend/html-report/index.html'])\"",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report e2e/test-results/html-report",
    "e2e:install": "playwright install",
    "lint": "ng lint",
    "analyze": "ng build --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "build:vercel": "ng build --configuration=production",
    "start:vercel": "npx vercel dev",
    "preview:vercel": "npx vercel build && npx vercel dev"
  },
  "private": true,
  "overrides": {
    "@typescript-eslint/utils": "7.11.0",
    "@typescript-eslint/type-utils": "7.11.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0"
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/service-worker": "^17.0.0",
    "@fortawesome/angular-fontawesome": "^0.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@materia-ui/ngx-monaco-editor": "^6.0.0",
    "@types/highlight.js": "^10.1.0",
    "bootstrap": "^5.3.0",
    "chart.js": "^4.4.0",
    "highlight.js": "^11.11.1",
    "lottie-web": "^5.12.0",
    "monaco-editor": "^0.52.2",
    "ng2-charts": "^5.0.0",
    "ngx-loading": "^17.0.0",
    "ngx-lottie": "^10.0.0",
    "ngx-toastr": "^17.0.0",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.7.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "17.5.2",
    "@angular-eslint/eslint-plugin": "17.5.2",
    "@angular-eslint/eslint-plugin-template": "17.5.2",
    "@angular-eslint/schematics": "17.5.2",
    "@angular-eslint/template-parser": "17.5.2",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@playwright/test": "^1.54.2",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "@typescript-eslint/utils": "7.11.0",
    "@typescript-eslint/type-utils": "7.11.0",
    "eslint": "8.57.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "playwright": "^1.54.2",
    "typescript": "~5.2.0"
  }
}