{
  "name": "frontuna-backend",
  "version": "1.0.0",
  "description": "Frontuna.ai Backend API Server",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "echo 'Using serverless functions - no build needed'",
    "dev": "ts-node --transpile-only src/server.ts",
    "dev:watch": "nodemon --exec ts-node --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "migrate": "node scripts/migrate.js",
    "seed": "ts-node --transpile-only scripts/seed.ts",
    "backup": "bash scripts/backup.sh",
    "etl": "ts-node --transpile-only scripts/mongo-to-neon.ts",
    "audit": "ts-node --transpile-only scripts/audit-data.ts",
    "docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.js -o docs/swagger.json",
    "copilot-guard": "node ai-copilot-guard.js",
    "copilot-scan": "node ai-copilot-guard.js",
    "auth-guardian": "node auth-copilot-guardian.js",
    "ultimate-auth-scan": "node auth-copilot-guardian.js"
  },
  "keywords": [
    "ai",
    "saas",
    "api",
    "nodejs",
    "express",
    "openai"
  ],
  "author": "Frontuna.ai Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/uuid": "^10.0.0",
    "archiver": "^5.3.1",
    "argon2": "^0.44.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^1.6.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.4",
    "openai": "^4.0.0",
    "otplib": "^12.0.1",
    "pg": "^8.11.3",
    "sharp": "^0.32.5",
    "socket.io": "^4.7.2",
    "unzipper": "^0.10.14",
    "uuid": "^11.1.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-clean": "^0.1.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prisma": "^6.13.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
